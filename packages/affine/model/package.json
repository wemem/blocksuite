{
  "name": "@blocksuite/affine-model",
  "version": "0.17.18",
  "description": "Models for BlockSuite in Affine.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/toeverything/blocksuite.git",
    "directory": "packages/affine/model"
  },
  "scripts": {
    "build": "tsc && yarn transform",
    "transform": "jscodeshift --run-in-band -t ../../../../../scripts/transform.cjs dist/",
    "test:unit": "nx vite:test --run --passWithNoTests",
    "test:unit:coverage": "nx vite:test --run --coverage",
    "test:e2e": "playwright test"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@blocksuite/block-std": "workspace:*",
    "@blocksuite/global": "workspace:*",
    "@blocksuite/inline": "workspace:*",
    "@blocksuite/store": "workspace:*",
    "fractional-indexing": "^3.2.0",
    "zod": "^3.23.8"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "files": [
    "src",
    "dist",
    "!src/__tests__",
    "!dist/__tests__"
  ],
  "devDependencies": {
    "jscodeshift": "^17.1.1"
  }
}
