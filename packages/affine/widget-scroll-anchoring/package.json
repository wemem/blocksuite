{
  "name": "@blocksuite/affine-widget-scroll-anchoring",
  "version": "0.17.18",
  "description": "Affine scroll anchoring widget.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/toeverything/blocksuite.git",
    "directory": "packages/affine/widget-scroll-anchoring"
  },
  "scripts": {
    "build": "tsc && yarn transform",
    "transform": "jscodeshift --run-in-band -t ../../../../../scripts/transform.cjs dist/",
    "test:unit": "nx vite:test --run --passWithNoTests",
    "test:unit:coverage": "nx vite:test --run --coverage",
    "test:e2e": "playwright test"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@blocksuite/affine-model": "workspace:*",
    "@blocksuite/affine-shared": "workspace:*",
    "@blocksuite/block-std": "workspace:*",
    "@blocksuite/global": "workspace:*",
    "@preact/signals-core": "^1.8.0",
    "@toeverything/theme": "^1.0.8",
    "lit": "^3.2.0"
  },
  "exports": {
    ".": "./src/index.ts",
    "./effects": "./src/effects.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./effects": {
        "import": "./dist/effects.js",
        "types": "./dist/effects.d.ts"
      }
    }
  },
  "files": [
    "src",
    "dist",
    "!src/__tests__",
    "!dist/__tests__"
  ],
  "devDependencies": {
    "jscodeshift": "^17.1.1"
  }
}
